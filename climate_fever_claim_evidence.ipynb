        "def tokenize(text):\n",
        "    tokens = nltk.word_tokenize(text.lower())\n",
        "    return [token for token in tokens if token.isalpha()]"
      ],
      "metadata": {
        "id": "E6dH4G3mpA8A"
      },
      "execution_count": null,
      "outputs": []
