        "\n",
        "    if val_macro_f1 > best_f1:\n",
        "        best_f1 = val_macro_f1\n",
        "        wait = 0\n",
        "        best_state = model.state_dict()\n",
        "    else:\n",
        "        wait += 1\n",
        "        if wait >= patience:\n",
        "            print(\"Early stopping triggered.\")\n",
        "            break\n",
        "\n",
        "model.load_state_dict(best_state)"
